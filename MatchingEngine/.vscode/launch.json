{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--config",
                "${workspaceFolder}/config/"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/Debug/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--config",
                "${workspaceFolder}/config/"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/Debug/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        }
    ]
}