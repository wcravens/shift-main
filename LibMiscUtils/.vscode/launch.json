    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch FileCryptor",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/FileCryptor",
            "args": [
                "--encrypt",
                "${workspaceFolder}/filecryptor/sample.txt"
            ],
            "cwd": "${workspaceFolder}/build/Debug/",
            "env": {},
            "terminal": "integrated",
            "stopOnEntry": false
        },
        {
            "name": "Launch GUIDTester",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/GUIDTester",
            "args": [],
            "cwd": "${workspaceFolder}/build/Debug/",
            "env": {},
            "terminal": "integrated",
            "stopOnEntry": false
        },
        {
            "name": "Launch TRTHRESTDownloader",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/TRTHRESTDownloader",
            "args": [
                "--jsondir",
                "${workspaceFolder}/trthrestdownloader/"
            ],
            "cwd": "${workspaceFolder}/build/Debug/",
            "env": {},
            "terminal": "integrated",
            "stopOnEntry": false
        }
    ]
}